{
  "publisher": "mohsen1",
  "name": "llm-debugger",
  "displayName": "LLM Debugger",
  "version": "0.0.0",
  "private": true,
  "volta": {
    "node": "20.18.1",
    "pnpm": "9.7.1"
  },
  "description": "This is a VSCode extension that allows you to debug your code using an LLM.",
  "author": "Mohsen Azimi <me@azimi.me>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/mohsen1/llm-debugger#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohsen1/llm-debugger"
  },
  "bugs": {
    "url": "https://github.com/mohsen1/llm-debugger/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/mohsen1"
  },
  "keywords": [
    "debug",
    "debugger",
    "llm",
    "ai",
    "openai"
  ],
  "categories": [
    "Other",
    "Debuggers"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.96.0"
  },
  "activationEvents": [
    "onDebugResolve:node",
    "onDebugAdapterProtocolTracker:pwa-node"
  ],
  "contributes": {
    "debuggers": [
      {
        "type": "pwa-node"
      }
    ],
    "commands": [
      {
        "command": "llm-debugger.startLLMDebug",
        "title": "Start LLM Debug Session"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "LLM Debugger",
      "properties": {}
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "prepare": "nr update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "dependencies": {
    "@vscode/debugadapter": "^1.68.0",
    "openai": "^4.82.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.26.0",
    "@antfu/ni": "^0.22.4",
    "@types/node": "^22.4.1",
    "@types/vscode": "1.96.0",
    "@vscode/vsce": "^3.0.0",
    "bumpp": "^9.5.1",
    "eslint": "^9.9.0",
    "esno": "^4.7.0",
    "reactive-vscode": "^0.2.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vitest": "^2.0.5",
    "vscode-ext-gen": "^0.4.1"
  }
}
