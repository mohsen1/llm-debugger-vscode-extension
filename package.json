{
  "publisher": "mohsen1",
  "name": "llm-debugger",
  "displayName": "LLM Debugger",
  "version": "0.0.0",
  "private": true,
  "volta": {
    "node": "20.18.1",
    "pnpm": "9.7.1"
  },
  "description": "This is a VSCode extension that allows you to debug your code using an LLM.",
  "author": "Mohsen Azimi <me@azimi.me>",
  "license": "MIT",
  "keywords": [
    "debug",
    "debugger",
    "llm",
    "ai",
    "openai"
  ],
  "categories": [
    "Debuggers"
  ],
  "main": "./out/index.js",
  "icon": "res/icon.png",
  "files": [
    "out/*",
    "LICENSE.md",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.96.0"
  },
  "activationEvents": [
    "onDebugResolve:node"
  ],
  "contributes": {
    "debuggers": [
      {
        "type": "node"
      }
    ],
    "commands": [
      {
        "command": "llm-debugger.startLLMDebug",
        "title": "Start LLM Debug Session"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "LLM Debugger",
      "properties": {}
    }
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --outdir=out --format=cjs --sourcemap --external:vscode --external:@vscode/* --minify=false --keep-names",
    "dev": "pnpm build --watch",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "vscode:prepublish": "tsc",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish"
  },
  "dependencies": {
    "@vscode/debugadapter": "^1.68.0",
    "chalk": "^5.4.1",
    "openai": "^4.82.0"
  },
  "devDependencies": {
    "@types/node": "^22.4.1",
    "@types/vscode": "1.96.0",
    "@vscode/vsce": "^3.0.0",
    "bumpp": "^9.5.1",
    "esbuild": "^0.24.2",
    "eslint": "^9.19.0",
    "reactive-vscode": "^0.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.23.0",
    "vite": "^5.4.1",
    "vitest": "^2.0.5",
    "vscode-ext-gen": "^0.4.1"
  }
}
